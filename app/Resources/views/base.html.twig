<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>
        {% block title %}
            Sant√© Vet - Annonces
        {% endblock %}
    </title>
    {% block stylesheets %}
        <link href="{{ asset('bundles/app/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
        <!-- MetisMenu CSS -->
        <link href="{{ asset('bundles/app/js/metisMenu/metisMenu.min.css') }}" rel="stylesheet">
        <!-- Custom CSS -->
        <link href="{{ asset('bundles/app/css/sb-admin-2.min.css') }}" rel="stylesheet">
        <!-- Morris Charts CSS -->
        <link href="{{ asset('bundles/app/js/morrisjs/morris.css') }}" rel="stylesheet">
        <!-- Custom Fonts -->
        <link href="{{ asset('bundles/app/js/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
<div id="wrapper">
    {% include 'nav.html.twig' %}


    <div id="page-wrapper">
        {% block flash_messages %}
            {% for flash_message in app.session.flashbag.get('success') %}
                <div class="alert alert-success">
                    <button type="button" class="close" data-dismiss="alert">
                        <i class="ace-icon fa fa-times"></i>
                    </button>
                    {{ flash_message }}
                </div>
            {% endfor %}
        {% endblock %}
        {% block body %}{% endblock %}
    </div>
</div>
{% block javascripts %}
    <script>window.jQuery || document.write('<script src="{{ asset('bundles/app/js/jquery/jquery.min.js') }}"><\/script>')</script>

    <script defer src="{{ asset('bundles/app/js/jquerytools.min.js') }}"></script>
    <script defer src="{{ asset('bundles/app/js/bootstrap.min.js') }}"></script>


    <!-- Metis Menu Plugin JavaScript -->
    <script src="{{ asset('bundles/app/js/metisMenu/metisMenu.min.js') }}"></script>

    <!-- Morris Charts JavaScript -->
    <script src="{{ asset('bundles/app/js/raphael/raphael.min.js') }}"></script>
    <script src="{{ asset('bundles/app/js/morrisjs/morris.min.js') }}"></script>

    <!-- FOSJSRouting -->
    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>

    <!-- Custom Theme JavaScript -->
    <script src="{{ asset('bundles/app/js/sb-admin-2.min.js') }}"></script>
    <script src="{{ asset('bundles/app/js/script.js') }}"></script>
{% endblock %}
</body>
</html>
